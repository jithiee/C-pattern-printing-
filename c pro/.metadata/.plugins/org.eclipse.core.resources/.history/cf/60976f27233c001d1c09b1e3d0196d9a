#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int j,i;
	for(i=1;i<=5;i++)
	{
		for(j=1;j<=10;j--)
		{
			printf("* ");
		}
		printf("\n");
	}
	return EXIT_SUCCESS;
}
