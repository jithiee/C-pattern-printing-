#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int j,i;
	for(i=1;i<=4;i++)
	{
		for(j=1;j<=5;j++)
		{
			printf("  ")
			printf("%c ",j+64);
		}
		printf("\n");
	}
	return EXIT_SUCCESS;
}
