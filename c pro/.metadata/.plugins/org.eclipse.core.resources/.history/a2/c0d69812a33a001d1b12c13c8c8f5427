#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int i,j;
	for(i=1;i<=6;i++)
	{
		for(j=6;j>=i;j--  )
		{
			printf("123 ");

		}
		printf("\n");
	}
	return EXIT_SUCCESS;
}
